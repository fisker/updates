      "bugs": { "url": "https://github.com/svgstore/svgstore/issues" },
      "homepage": "https://github.com/svgstore/svgstore",
      "dependencies": { "cheerio": "^0.22.0", "object-assign": "^4.1.0" },
      "devDependencies": {
        "ava": "0.16.0",
        "chokidar-cli": "^1.2.0",
        "coveralls": "^2.11.12",
        "nyc": "^8.1.0",
